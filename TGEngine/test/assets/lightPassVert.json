{
 "settings": {
  "shaderType": "vertex"
 },
 "codes": [
  {
   "code": [
    "#version 450",
    "#extension GL_KHR_vulkan_glsl: enable",
    "out gl_PerVertex {",
    "vec4 gl_Position;",
    "};",
    "void main() {",
    "gl_Position = vec4(vec2((gl_VertexIndex << 1) & 2, gl_VertexIndex & 2) * 2.0f - 1.0f, 0.0f, 1.0f);",
    "}"
   ]
  }
 ]
}